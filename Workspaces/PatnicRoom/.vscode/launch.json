    "version": "0.2.0",
    "configurations": [
        {
            "name":"play",
            "type": "godot",
            "request": "launch",
            "project": "${workspaceRoot}",
            "port": 9999,
            "address": "127.0.0.1",
            "launch_game_instance": true,
            "launch_scene": false
        },
        {
            "name": "Play in Editor",
            "type": "godot-mono",
            "mode": "playInEditor",
            "request": "launch"
        },
        {
            "name": "Launch",
            "type": "godot-mono",
            "request": "launch",
            "mode": "executable",
            "executable": "C:/Users/Daniel/Desktop/godot.exe",
            "executableArguments": [
                "--path",
                "${workspaceRoot}"
            ]
        },
        {
            "name": "Attach",
            "type": "godot-mono",
            "request": "attach",
            "address": "localhost",
            "port": 6007
        }
        
    ]
}